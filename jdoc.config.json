{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": "./resource/js/Putity",
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "destination": "./jsDoc/",
        "recurse": true
    }
}